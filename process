  # 1. 停止并删除旧容器
docker rm -f security-gui

# 2. 重新启动，加上 --privileged 和 TUN 设备映射
docker run -d \
  --name security-gui \
  --privileged \
  --device /dev/net/tun:/dev/net/tun \
  -p 3000:3000 \
  -p 2222:22 \
  -v /home/$USER/data:/config \
  --shm-size="1gb" \
  --restart unless-stopped \
  linuxserver/webtop:ubuntu-xfce



  import os

# List of proxy variables to remove
proxy_vars = [
    'http_proxy', 'https_proxy', 'all_proxy',
    'HTTP_PROXY', 'HTTPS_PROXY', 'ALL_PROXY'
]

for var in proxy_vars:
    if var in os.environ:
        del os.environ[var]
        print(f"Unset: {var}")
    else:
        print(f"Skipped: {var} (not set)")